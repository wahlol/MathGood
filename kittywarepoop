local Auto = X.New({
	Title = "Auto Farm"
})



local D = Auto.Dropdown({
	Text = "Select Lot!",
	Callback = function(Value)
	
	if Value == "1" then
	    
	   LocalPlot = 1
	   
	   elseif Value == "2" then
	    
	   LocalPlot = 2
	   
	   elseif Value == "3" then
	    
	   LocalPlot = 3
	   
	   elseif Value == "4" then
	    
	   LocalPlot = 4
	   
	   elseif Value == "5" then
	    
	   LocalPlot = 5
	   
	   elseif Value == "6" then
	    
	   LocalPlot = 6
	    
	end
	end,
	Options = {
		"1",
		"2",
		"3",
		"4",
		"5",
		"6"
    }
	


})

local A = Auto.Button({
	Text = "Instant Use Trash",
	Callback = function()
game:GetService("ReplicatedStorage").Communication.UseTrash:FireServer()

	end
})


local A = Auto.Button({
	Text = "Get Lemondade",
	Callback = function()

if LocalPlot == 6 then

game:GetService("ReplicatedStorage").Communication.UseMachine:FireServer(workspace.Plots:FindFirstChild("6").Objects.Blender1, true)

elseif LocalPlot == 1 then

game:GetService("ReplicatedStorage").Communication.UseMachine:FireServer(workspace.Plots:FindFirstChild("1").Objects.Blender1, true)

elseif LocalPlot == 2 then

game:GetService("ReplicatedStorage").Communication.UseMachine:FireServer(workspace.Plots:FindFirstChild("2").Objects.Blender1, true)


elseif LocalPlot == 3 then

game:GetService("ReplicatedStorage").Communication.UseMachine:FireServer(workspace.Plots:FindFirstChild("3").Objects.Blender1, true)

